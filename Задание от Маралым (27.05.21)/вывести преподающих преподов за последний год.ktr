  <step>
    <name>Get all employees wiht pps position</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>hr</connection>
    <sql> /*Запрос получает последний приказ на сотрудника.*/
 /*Запрос получает последний приказ на сотрудника.*/
 
WITH summary AS (SELECT 
		emp.[Id] 'EmployeeID'
      ,[FirstName]
      ,[MiddleName]
      ,[LastName]
      ,[Iin]
      ,ord.[DateIssued] 'LastOrder DateIssued'
	  ,ord.TypeId 'OrderId'
		,ordEmp.TariffRateId
	  ,dictOrd.Title 'OrderName'
	  ,pos.[Id] 'PositionId'
	  ,pos.Title 'PositionName'
	  ,dep.Title
	  ,ROW_NUMBER() OVER (PARTITION BY emp.[Id] ORDER BY ord.[DateIssued] DESC) AS rk
			  FROM [kaznitu_hr].[dbo].[Orders] ord
			  LEFT OUTER JOIN
			  [kaznitu_hr].[dbo].[OrderEmployees] ordEmp
			  ON (ord.Id = ordEmp.OrderId)
			  LEFT OUTER JOIN
			  [kaznitu_hr].[dbo].[Employees] emp
			  ON (emp.Id = ordEmp.EmployeeId)
			  LEFT OUTER JOIN [kaznitu_hr].[dbo].[SimpleDictionaries] dictOrd
			  ON (dictOrd.Id = ord.TypeId)
			  LEFT OUTER JOIN [kaznitu_hr].[dbo].[Positions] pos
			  ON (ordEmp.PositionId = pos.Id)
			  LEFT OUTER JOIN [kaznitu_hr].[dbo].[Departments] dep
			  ON (ordEmp.DepartmentId = dep.Id)
		  )

		  SELECT s.*
		  FROM summary s
WHERE s.rk = 1
ORDER BY s.LastName
/*TariffRateId - это ставка, есть в SimpleDictionaries, 22 - полная ставка*/</sql>
