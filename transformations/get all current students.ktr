    <connection>sso</connection>
    <sql>/****** Скрипт для команды SelectTopNRows из среды SSMS  ******/
SELECT [StudentID]
      ,[SpecialityID]
      ,[StatusID]
	  ,LastName
	  ,FirstName
	  ,[MiddleName]
      ,[PersonalEmail]
      ,[DOB]
      ,[PlaceOfBirth]
      ,[NationalityID]
      ,[Male]
      ,[CitizenshipCountryID]
      ,[IIN]
      ,us.[LastUpdatedBy]
      ,us.[LastUpdatedOn]
      ,[CitizenCategoryID]
      ,[Resident]
      ,[CategoryID]
      ,[NeedsDorm]
      ,[AltynBelgi]
      ,[EducationTypeID]
      ,[EducationPaymentTypeID]
      ,[GrantTypeID]
      ,[EducationDurationID]
      ,[Year]
      ,[StudyLanguageID]
      ,[RupID]
      ,[EntryDate]
      ,[GPA]
      ,st.[LastUpdatedBy]
      ,st.[LastUpdatedOn]
      ,[AdvisorID]
      ,[AcademicStatusID]
      ,[GPA_Y]
      ,[HosterPrivelegeID]
  FROM [KazNITU].[dbo].[Edu_Students] st
  LEFT OUTER JOIN
  [KazNITU].[dbo].[Edu_Users] us
  ON us.ID = st.StudentID
  WHERE StatusID IS NULL
  AND LastName != ''
  AND LastName NOT LIKE '%[0-9]%'
  AND FirstName NOT LIKE '%[0-9]%'
  AND StudentID NOT IN 
