   ,us.MiddleName
   ,st.OrderID
   ,od.OrderTypeID
   ,ftp.OrderFunctionalityTypeID
   ,od.OrderDate
   ,tp.Title
  FROM [KazNITU].[dbo].[Edu_OrderStudents] AS st
  RIGHT OUTER JOIN [KazNITU].[dbo].[Edu_Orders] AS od
