    <connection>postgres</connection>
    <sql>SELECT 
f_author_id, f_treatise_name, 
f_issue_year, f_isbn, f_doi_treatise, 
f_from_agris, f_from_astrophysica, 
f_from_georef, f_from_ijournal, 
f_from_math_scinet, f_from_pubmed, 
f_from_scopus, f_from_wos,
f_from_zbmath, f_mon_rk, f_publication_type,
f_source_title, f_author_pid, COUNT(*)
FROM public.tb_treatise
GROUP BY f_author_id, f_treatise_name, 
f_issue_year, f_isbn, f_doi_treatise, 
f_from_agris, f_from_astrophysica, 
f_from_georef, f_from_ijournal, 
f_from_math_scinet, f_from_pubmed, 
f_from_scopus, f_from_wos,
f_from_zbmath, f_mon_rk, f_publication_type,
f_source_title, f_author_pid
HAVING COUNT(*) >= 2
ORDER BY COUNT(*) DESC;</sql>
